{
    "buildCommand": "cd intelli-cater && npm install && npm run build",
    "outputDirectory": "intelli-cater/dist",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
